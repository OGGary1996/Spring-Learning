<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "https://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.transactionAnnotation.mapper.AccountMapper">
    <select id="selectList" resultType="com.transactionAnnotation.entity.Account">
        SELECT * from account
    </select>

    <update id="update" >
        UPDATE account
        SET balance = balance + #{money}
        WHERE id = #{id}
    </update>


</mapper>
